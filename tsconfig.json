{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "dom",
            "es2015"
        ]
    },
    "typedocOptions": {
        "include": [
            "../sprest-def/lib/SP/**/*"
        ],
        "exclude": [
            "../sprest/@types/index.d.ts",
            "../sprest/@types/intellisense/index.d.ts",
            "../sprest/@types/intellisense/graph.d.ts",
            "../sprest/@types/lib/graph.d.ts",
            "../sprest/@types/sptypes/index.d.ts",
            "../sprest/@types/utils/**/*"
        ],
        "excludeExternals": true,
        "includeDeclarations": true,
        "mode": "modules",
        "theme": "./theme"
    }
}