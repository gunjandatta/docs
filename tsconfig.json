{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "dom",
            "es2015"
        ]
    },
    "typedocOptions": {
        "inputFiles": [
            "@types",
            "lib/SP/entitytypes.d.ts"
        ],
        "exclude": [
            "@types/intellisense/index.d.ts",
            "@types/intellisense/graph.d.ts",
            "@types/lib/graph.d.ts",
            "@types/sptypes/index.d.ts",
            "@types/utils/**/*"
        ],
        "excludeExternals": true,
        "includeDeclarations": true,
        "mode": "modules",
        "out": "docs/sprest",
        "theme": "./theme"
    }
}